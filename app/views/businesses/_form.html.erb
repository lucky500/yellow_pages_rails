<%= render('layouts/errors', :object => @business) %>
  <%= form_for [@category, @business] do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <%= f.label :zip %>
    <%= f.text_field :zip %>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
    <%= f.label :website %>
    <%= f.text_field :website %>
    <p><%= f.submit %></p>
<% end %>
